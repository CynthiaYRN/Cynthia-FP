int main()
{
	int temp;
	cout << "temperatura: ";
	cin >> temp;
	cout << endl;
	if (temp < 10)
	{
		cout
			<< "Hace mucho frÃ­o "
			<< endl;

	}
	if (temp >= 10 and
		temp <= 17)
	{
		cout << "frio " << endl;
	}
	if (temp > 17)
	{ 
		cout << "templado"
			<< endl;
	}
}


